    "partyserver": "^0.1.5",
    "phaser": "^3.90.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@types/node": "^25.2.3",
    "discord-api-types": "^0.38.39",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "wrangler": "^4.66.0"
  }
